export default function (sequelize, DataTypes) {
    let Sample = sequelize.define("Sample", {
        title: DataTypes.STRING
    });

    return Sample;
};

export default function (sequelize, DataTypes) {
    let Activity = sequelize.define("Activity", {
        act_id: DataTypes.STRING,
        act_name: DataTypes.STRING,
        act_desc: DataTypes.STRING
    });

    return Activity;
};
